{
  "env": {
    "modules": false,
    "targets": {
      "browsers": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
      ]
    }
  },
  "presets": [
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    ["component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]]
  ]
}